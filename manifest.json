{
  "manifest_version": 3,
  "name": "DeepSeek AI Chatbot Extension",
  "version": "1.0",
  "description": "A chatbot powered by DeepSeek AI.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://deepseekaichatbot.vercel.app/"
  ]
}
